/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a, out=aNot);
    Not(in=b, out=bNot);
    And(a=a, b=bNot, out=tmp1);
    And(a=b, b=aNot, out=tmp2);
    Or(a=tmp1, b=tmp2, out=out);
}